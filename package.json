{
  "name": "tiny-utm-keeper",
  "version": "1.0.2",
  "description": "Zero-dependency, TypeScript-first UTM parameter tracker for first-touch/last-touch attribution. SSR-friendly.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run build:types",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs --declaration false",
    "build:esm": "tsc --module esnext --outDir dist/esm",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist/types",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "utm",
    "utm-tracking",
    "attribution",
    "analytics",
    "first-touch",
    "last-touch",
    "typescript",
    "nextjs",
    "ssr",
    "zero-dependency"
  ],
  "author": "Amir Hossein",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/amirhossein693/tiny-utm-keeper.git"
  },
  "bugs": {
    "url": "https://github.com/amirhossein693/tiny-utm-keeper/issues"
  },
  "homepage": "https://github.com/amirhossein693/tiny-utm-keeper#readme",
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}

